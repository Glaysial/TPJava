<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MailController">
   <top>
      <Label fx:id="application" text="Nom de l'application" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </padding></Label>
   </top>
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="NbrMailBloque" prefHeight="67.0" prefWidth="350.0" text="NbrBloque">
               <graphic>
                  <Label text="Nombre de mail bloqué : ">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Label>
               </graphic>
               <VBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="NbrMail" prefHeight="18.0" prefWidth="300.0" text="NbrMail">
               <graphic>
                  <Label text="Nombre de mail traité : ">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Label>
               </graphic>
               <VBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="PourcentageBloque" prefHeight="55.0" prefWidth="300.0" text="PourcentageBloque" textAlignment="CENTER">
               <graphic>
                  <Label text="Pourcentage de mail bloqué : ">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Label>
               </graphic>
               <VBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </center>
</BorderPane>
